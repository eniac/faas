#!/bin/bash

echo "Emailing results"
sendemail -f results@faas.com -m "Please see the log file for factorization results." -t {{ supervisor.email }} -u "FACTORING FINISHED" -a "{{ workdir }}/{{ cado.name }}/{{ cado.name }}.log" -a "/home/ubuntu/server.stderr"

echo "Stopping master node"
ansible-playbook -i {{ faasdir_remote }}/ec2/ec2.py {{ faasdir_remote }}/ec2/stop-master.yml
